#calling objects
import webbrowser
import time

#storing link
url="https://youtu.be/REoANd69pOI" 

#storing time in seconds
seconds=int(10)

time.sleep(seconds)

webbrowser.open(url)

#Playing music at appointed time
while time:
    print ("lets relax massa")
    time.sleep(seconds)
    webbrowser.open(url)
